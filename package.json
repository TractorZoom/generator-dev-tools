{
    "name": "@tractorzoom/generator-dev-tools",
    "version": "2.6.0",
    "description": "Yeoman generator to add dev tools and configurations to node projects",
    "author": "@TractorZoom",
    "license": "MIT",
    "engines": {
        "node": "12.x"
    },
    "files": [
        "generators"
    ],
    "scripts": {
        "commit": "git-cz",
        "prettier:all": "prettier --write './**/*.*'",
        "pretty-quick": "pretty-quick",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "dependencies": {
        "fs-extra": "9.0.1",
        "replace-in-file": "6.1.0",
        "yeoman-generator": "4.10.1"
    },
    "devDependencies": {
        "@semantic-release/changelog": "5.0.1",
        "@semantic-release/git": "9.0.0",
        "@semantic-release/release-notes-generator": "9.0.1",
        "semantic-release": "17.2.3",
        "@commitlint/cli": "8.3.5",
        "@commitlint/config-conventional": "8.3.4",
        "commitizen": "4.1.2",
        "husky": "4.2.5",
        "prettier": "2.0.5",
        "pretty-quick": "2.0.1"
    },
    "config": {
        "commitizen": {
            "path": "cz-conventional-changelog"
        }
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/TractorZoom/generator-dev-tools.git"
    },
    "homepage": "https://github.com/TractorZoom/generator-dev-tools",
    "bugs": {
        "url": "https://github.com/TractorZoom/generator-dev-tools/issues"
    },
    "keywords": [
        "dev-tools",
        "developer",
        "developer-tools",
        "yeoman",
        "yeoman-generator"
    ]
}
